{
  "name": "genpw",
  "version": "1.0.9",
  "description": "Generate a password.",
  "homepage": "https://genpw.com",
  "bugs": {
    "url": "https://github.com/genpw/genpw/issues"
  },
  "license": "MIT",
  "author": "Matthew <matthew@mi11er.net> (https://mi11er.net)",
  "files": ["dist/*", "LICENSE", "package.json", "README.md", "CHANGELOG.md"],
  "main": "./dist/main/passwordgenerator.js",
  "module": "./dist/module/passwordgenerator.js",
  "bin": {
    "genpw": "./dist/main/exec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/genpw/genpw.git"
  },
  "scripts": {
    "build": "run-s clean test build:*",
    "build:main": "BABEL_ENV=main babel -d dist/main/ src/js/lib/",
    "build:module": "BABEL_ENV=module babel -d dist/module/ src/js/lib/",
    "clean": "rimraf .nyc_output coverage dist/* npm-debug.log",
    "precommit": "run-s lint-staged pretty-quick",
    "lint": "eslint --fix .",
    "lint-staged": "lint-staged",
    "semantic-release": "semantic-release",
    "pretest": "run-s lint",
    "test": "BABEL_ENV=main nyc mocha",
    "posttest": "BABEL_ENV=main nyc report --reporter=lcov",
    "pretty-quick": "pretty-quick --staged"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "env": {
      "module": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current",
                "browsers": ["last 2 versions"]
              },
              "modules": false
            }
          ]
        ]
      },
      "main": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current",
                "browsers": ["last 2 versions"]
              },
              "modules": "commonjs"
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "sjcl": "1.0.7"
  },
  "devDependencies": {
    "@mi11er/eslint-config": "2.0.0",
    "@semantic-release/changelog": "1.0.1",
    "@semantic-release/git": "3.0.1",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "codeclimate-test-reporter": "0.5.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.18.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-no-unsafe-innerhtml": "1.0.16",
    "eslint-plugin-optimize-regex": "1.1.5",
    "eslint-plugin-scanjs-rules": "0.2.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "0.14.3",
    "lint-staged": "7.0.0",
    "mocha": "5.0.1",
    "npm-run-all": "4.1.2",
    "nyc": "11.4.1",
    "prettier": "1.10.2",
    "pretty-quick": "1.4.1",
    "rimraf": "2.6.2",
    "semantic-release": "13.0.1",
    "sort-package-json": "1.9.0"
  },
  "nyc": {
    "include": ["src/"],
    "require": ["babel-register"],
    "all": true
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "publish": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/npm",
        "tarballDir": "release"
      },
      {
        "path": "@semantic-release/git",
        "assets": ["**"]
      },
      {
        "path": "@semantic-release/github",
        "assets": [
          {
            "path": "release/*.tgz",
            "label": "Package tarball"
          }
        ]
      }
    ]
  }
}
